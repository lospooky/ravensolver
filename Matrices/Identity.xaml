            <Rectangle Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="ProblemCell2" Canvas.Left="200" Canvas.Top="0" Height="200" Width="200">
            <Rectangle Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="ProblemCell3" Canvas.Left="400" Canvas.Top="0" Height="200" Width="200">
            <Rectangle Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="ProblemCell4" Canvas.Left="0" Canvas.Top="200" Height="200" Width="200">
            <Ellipse Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="ProblemCell5" Canvas.Left="200" Canvas.Top="200" Height="200" Width="200">
            <Ellipse Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="ProblemCell6" Canvas.Left="400" Canvas.Top="200" Height="200" Width="200">
            <Ellipse Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="ProblemCell7" Canvas.Left="0" Canvas.Top="400" Height="200" Width="200">
            <Polygon Canvas.Top="67.525" Canvas.Left="62" Points="0,64.95 75,64.95 37.5,0 " Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="ProblemCell8" Canvas.Left="200" Canvas.Top="400" Height="200" Width="200">
            <Polygon Canvas.Top="67.525" Canvas.Left="62" Points="0,64.95 75,64.95 37.5,0 " Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="SolutionCell" Canvas.Left="400" Canvas.Top="400" Height="200" Width="200"></Canvas>
    </Canvas>

    
    <Canvas x:Name="AnswersGrid" Canvas.Left="0" Canvas.Top="750">
        <Canvas x:Name="AnswerCell1" Canvas.Left="0" Canvas.Top="0" Height="200" Width="200">
            <Ellipse Canvas.Top="50" Canvas.Left="50" Height="100" Width="100" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="25" Canvas.Left="25" Height="150" Width="150" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="AnswerCell2" Canvas.Left="200" Canvas.Top="0" Height="200" Width="200">
            <Ellipse Canvas.Top="87.5" Canvas.Left="87.5" Height="25" Width="25" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="72.5" Canvas.Left="72.5" Height="55" Width="55" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="55" Canvas.Left="55" Height="90" Width="90" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="37.5" Canvas.Left="37.5" Height="125" Width="125" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="20" Canvas.Left="20" Height="160" Width="160" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="AnswerCell3" Canvas.Left="400" Canvas.Top="0" Height="200" Width="200">
            <Ellipse Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="50" Canvas.Left="50" Height="100" Width="100" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="AnswerCell4" Canvas.Left="600" Canvas.Top="0" Height="200" Width="200">
            <Ellipse Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="AnswerCell5" Canvas.Left="0" Canvas.Top="200" Height="200" Width="200">
            <Ellipse Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="25" Canvas.Left="25" Height="150" Width="150" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="AnswerCell6" Canvas.Left="200" Canvas.Top="200" Height="200" Width="200">
            <Ellipse Canvas.Top="25" Canvas.Left="25" Height="150" Width="150" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="AnswerCell7" Canvas.Left="400" Canvas.Top="200" Height="200" Width="200">
            <Ellipse Canvas.Top="50" Canvas.Left="50" Height="100" Width="100" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
        <Canvas x:Name="AnswerCell8" Canvas.Left="600" Canvas.Top="200" Height="200" Width="200">
            <Ellipse Canvas.Top="75" Canvas.Left="75" Height="50" Width="50" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="50" Canvas.Left="50" Height="100" Width="100" Stroke="Black" StrokeThickness="2"/>
            <Ellipse Canvas.Top="25" Canvas.Left="25" Height="150" Width="150" Stroke="Black" StrokeThickness="2"/>
        </Canvas>
    </Canvas>
    
    <Canvas Canvas.ZIndex="-1000"  x:Name="Background" Width="800" Height="1150">
        <Canvas x:Name="ProblemBackgroundGrid">
            <TextBlock Text="Problem" Canvas.Left="100" Canvas.Top="22" FontFamily="Segoe UI" FontSize="18" TextAlignment="Left"/>
            <Rectangle Width="602" Height="602" Canvas.Left="99" Canvas.Top="49" Fill="#FFFFFF" Stroke="#000000" StrokeThickness="2"/>
            <Path Width="1" Height="601" Canvas.Left="299.5" Canvas.Top="49.500" Data="F1 M 300,50.000L 300,650" Stretch="Fill" Stroke="#000000"/>
            <Path Width="1" Height="601" Canvas.Left="499.5" Canvas.Top="49.5" Data="F1 M 500,50L 500,650" Stretch="Fill" Stroke="#000000"/>
            <Path Width="601" Height="1" Canvas.Left="99.5" Canvas.Top="249.5" Data="F1 M 99.999,250L 700,250" Stretch="Fill" Stroke="#000000"/>
            <Path Width="601" Height="1" Canvas.Left="99.499" Canvas.Top="449.5" Data="F1 M 99.999,450L 700,450" Stretch="Fill" Stroke="#000000"/>
        </Canvas>
        <Canvas x:Name="AnswerAlternativesBackgroundGrid">
            <TextBlock Text="Answer Alternatives" Canvas.Left="5" Canvas.Top="722" FontFamily="Segoe UI" FontSize="18" TextAlignment="Left"/>
            <Rectangle Width="802" Height="402" Canvas.Left="-1" Canvas.Top="749" Fill="#FFFFFF" Stretch="Fill" Stroke="#000000" StrokeLineJoin="Round" StrokeThickness="2"/>
            <Path Width="1" Height="401" Canvas.Left="199.5" Canvas.Top="749.5" Data="F1 M 200,750L 200,1150" Stretch="Fill" Stroke="#000000"/>
            <Path Width="1" Height="401" Canvas.Left="399.5" Canvas.Top="749.5" Data="F1 M 400,750L 400,1150" Stretch="Fill" Stroke="#000000"/>
            <Path Width="1" Height="401" Canvas.Left="599.5" Canvas.Top="749.5" Data="F1 M 600,750L 600,1150" Stretch="Fill" Stroke="#000000"/>
            <Path Width="801" Height="1" Canvas.Left="-0.499" Canvas.Top="949.5" Data="F1 M 8.831e-007,950L 800,950" Stretch="Fill" Stroke="#000000"/>
        </Canvas>
    </Canvas>
</Canvas>
